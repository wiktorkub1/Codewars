def DNA_strand(dna):
    result = ""

    for x in dna:
        if x == 'A':
            result += 'T'
        elif x == 'T':
            result += 'A'
        elif x == 'C':
            result += 'G'
        elif x == 'G':
            result += 'C'

    return result


if __name__ == '__main__':
    name_string = "ATGC"

    print(DNA_strand(name_string))
